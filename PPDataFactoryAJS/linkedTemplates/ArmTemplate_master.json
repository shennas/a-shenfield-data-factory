{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "PPDataFactoryAJS"
		},
		"ppd_lake_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ppd_lake'"
		},
		"ppd_lake_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://ppddatalake.dfs.core.windows.net/"
		},
		"trgTest_properties_copy_test_file_parameters_srcFolder": {
			"type": "string",
			"defaultValue": "@triggerBody().folderPath"
		},
		"trgTest_properties_copy_test_file_parameters_srcFile": {
			"type": "string",
			"defaultValue": "@triggerBody().fileName"
		},
		"trgTest_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/b2586655-8b7b-42af-aa3b-eb099ced9b9f/resourceGroups/PPD/providers/Microsoft.Storage/storageAccounts/ppddatalake"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "PPDataFactoryAJS_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ppd_lake_accountKey": {
						"value": "[parameters('ppd_lake_accountKey')]"
					},
					"ppd_lake_properties_typeProperties_url": {
						"value": "[parameters('ppd_lake_properties_typeProperties_url')]"
					},
					"trgTest_properties_copy_test_file_parameters_srcFolder": {
						"value": "[parameters('trgTest_properties_copy_test_file_parameters_srcFolder')]"
					},
					"trgTest_properties_copy_test_file_parameters_srcFile": {
						"value": "[parameters('trgTest_properties_copy_test_file_parameters_srcFile')]"
					},
					"trgTest_properties_typeProperties_scope": {
						"value": "[parameters('trgTest_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}